{
  "name": "wui-web",
  "description": "WUI components for Web Site",
  "author": "Cloud Mu",
  "user": "corncandy",
  "version": "0.1.5",
  "scripts": {
    "styles": "cat ./styles/*.css > ./dist/wui-web.min.css",
    "hbs": "handlebars ./templates/*.hbs -f ./templates/templates.js -e hbs -c handlebars/runtime",
    "start": "webpack-dev-server",
    "test": "mocha --recursive test",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --ignore-pattern templates --cache",
    "coverage": "istanbul cover _mocha --recursive test -- -R spec",
    "gh-pages": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --profile --json > stats.json",
    "dist": "npm run styles && npm run hbs && webpack",
    "dist:min": "npm run styles && npm run hbs && webpack",
    "dist:modules": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "pretest.bak": "npm run test:lint",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.9.0",
    "catalog": "^2.4.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "git-prepush-hook": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "html-webpack-template": "^5.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.0",
    "jsdom": "9.5.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "purecss": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-ghfork": "^0.3.5",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corncandy/wui-web.git"
  },
  "homepage": "https://corncandy.github.io/wui-web/",
  "bugs": {
    "url": "https://corncandy.github.io/wui-web/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "pre-push": [
    "test",
    "test:lint"
  ],
  "dependencies": {
    "handlebars": "^3.0.3",
    "xss": "^0.2.13"
  }
}
